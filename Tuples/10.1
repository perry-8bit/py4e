fname = input('Please enter file name: ')
try:
    fhand = open(fname)
except:
    print('File cannot be found!')
    exit()

count = dict()
for line in fhand:
    if line.startswith('From'):
        line = line.rstrip()
        words = line.split()
        if len(words) > 2:
            name = words[1]
            count[name] = count.get(name,0) + 1

#print(count)
lst = []
for key, values in count.items():
    lst.append((values, key))
lst.sort(reverse=True)
print(lst[0])
